{
  "name": "puppeteer-extra-plugin-repl",
  "version": "2.3.0",
  "description": "Start an interactive REPL in your puppeteer code.",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "berstend/puppeteer-extra",
  "author": "nswbmw & berstend",
  "license": "MIT",
  "scripts": {
    "dummy": "node -e 0",
    "clean": "rimraf index.{js,d.ts,js.map} lib/REPLSession.{js,d.ts,js.map} lib/super-readline.{js,d.ts,js.map}",
    "prebuild": "run-s clean",
    "docs": "node -e 0",
    "build": "tsc --module commonjs",
    "test": "ava",
    "test-ci": "run-s test"
  },
  "engines": {
    "node": ">=8"
  },
  "ava": {
    "files": [
      "*.test.js",
      "lib/*.test.js"
    ],
    "concurrency": 1,
    "verbose": true,
    "failFast": true
  },
  "keywords": [
    "puppeteer",
    "puppeteer-extra",
    "puppeteer-extra-plugin",
    "repl",
    "debug",
    "interactive",
    "puppeteer-debug",
    "puppeteer-repl",
    "chrome",
    "headless",
    "pupeteer"
  ],
  "devDependencies": {
    "@types/node": "^17.0.41",
    "ava": "3.15.0",
    "mock-stdin": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "puppeteer": "13.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "debug": "^4.1.1",
    "ow": "^0.4.0",
    "puppeteer-extra-plugin": "^3.2.0"
  },
  "gitHead": "babb041828cab50c525e0b9aab02d58f73416ef3"
}
