{
  "name": "puppeteer-extra-plugin-anonymize-ua",
  "version": "2.4.4",
  "description": "Anonymize User-Agent in puppeteer.",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "berstend/puppeteer-extra",
  "author": "berstend",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf index.{js,d.ts,js.map}",
    "prebuild": "run-s clean",
    "docs": "node -e 0",
    "build": "tsc --module commonjs",
    "test:js": "ava",
    "test": "run-p test:js",
    "test-ci": "run-s test"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "puppeteer",
    "puppeteer-extra",
    "puppeteer-extra-plugin",
    "ua",
    "user-agent",
    "chrome",
    "headless",
    "pupeteer"
  ],
  "ava": {
    "concurrency": 1,
    "timeout": "60s",
    "verbose": true,
    "environmentVariables": {
      "TS_NODE_COMPILER_OPTIONS": "{\"module\":\"commonjs\"}",
      "TS_NODE_FILES": "true",
      "TS_NODE_PROJECT": "./tsconfig.json"
    },
    "files": [
      "test/*.test.js"
    ],
    "extensions": [
      "js",
      "ts",
      "mts",
      "cts"
    ],
    "require": [
      "ts-node/register"
    ],
    "failFast": true
  },
  "devDependencies": {
    "@types/chrome": "^0.0.191",
    "ava": "3.15.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^2.0.0",
    "puppeteer-extra": "^3.3.4",
    "puppeteer-extra-plugin-devtools": "^2.4.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "puppeteer-extra-plugin": "^3.2.2"
  },
  "peerDependencies": {
    "playwright-extra": "*",
    "puppeteer-extra": "*"
  },
  "peerDependenciesMeta": {
    "puppeteer-extra": {
      "optional": true
    },
    "playwright-extra": {
      "optional": true
    }
  },
  "gitHead": "babb041828cab50c525e0b9aab02d58f73416ef3"
}
